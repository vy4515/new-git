import turtle as t
t.shape("turtle")
t.pensize(5)

#네모그리기
t.pencolor("blue")
for i  in range(4):
    t.forward(200)
    t.left(90)
     
#삼각형그리기
t.pencolor("yellow")
for i in range(3):
    t.forward(200)
    t.left(120)

#원그리기
t.pencolor("red")
t.circle(100)

#다각형 그리기
